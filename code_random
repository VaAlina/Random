<input type="button" onclick="pow()"/>

<script type='text/javascript'> 
function pow() {
  var b = document.getElementById("val").value;
  var a = Math.pow(10,b);

  var one = Math.round(Math.random()* a);
  var two = Math.round(Math.random()* a);
  var three = Math.round(Math.random()* a);
  alert(one + " " + two+" "+ three);
//var one, two, three = integer;
//var varn == one.length && two.length && three.length;
}
</script>
</body>
</html>






