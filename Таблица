        <input type = "button" value = "make it random" onclick =" 
qwe = Math.round(Math.random()* 100); //Задание значения переменной.
rty = Math.round(Math.random()* 100); //При добавлении перед переменной var 
uio = Math.round(Math.random()* 100); //код не работает. 
pas = Math.round(Math.random()* 100);
dfg = Math.round(Math.random()* 100);
hjk = Math.round(Math.random()* 100);
klz = Math.round(Math.random()* 100);
lzx = Math.round(Math.random()* 100); 
zxc = Math.round(Math.random()* 100);
xcv = Math.round(Math.random()* 100);
cvb = Math.round(Math.random()* 100); 
xcm = Math.round(Math.random()* 100);
cvx = Math.round(Math.random()* 100); 
vbn = Math.round(Math.random()* 100);
$('#qqq').text(qwe);// Присваивание ячейкам таблицы переменной,
$('#iii').text(rty);// которая будет та отображаться.
$('#fff').text(uio);
$('#xxx').text(pas);
$('#aaa').text(klz);
$('#sss').text(lzx);
$('#ddd').text(xcv);
$('#ggg').text(cvb);
$('#hhh').text(xcm);
$('#jjj').text(cvx);
$('#zzz').text(vbn);"/> 

